{
	"extends": [
		"next/core-web-vitals",
		"next/typescript",
		"plugin:prettier/recommended"
	],
	"plugins": ["prettier", "tailwindcss"],
	"rules": {
		"quotes": ["error", "double"],
		"prettier/prettier": [
			"error",
			{
				"semi": true,
				"singleQuote": false,
				"tabWidth": 4,
				"printWidth": 85,
				"trailingComma": "none"
			}
		  ]
	}
}
